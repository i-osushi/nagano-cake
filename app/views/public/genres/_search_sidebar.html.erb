<!--ジャンル検索サイドバー-->


    <table class="table table-bordered text-center ec-genre-list" >
  <thead>
  <tr>
    <th>ジャンル検索</th>
  </tr>
  </thead>
  <tbody>
  <% genres.each do |genre| %>
    <tr>
      <td><%= link_to genre.name, genre_path(genre.id) %></td>
    </tr>
  <% end %>
  </tbody>
</table>

