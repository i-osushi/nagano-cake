        <tr>
          <th class="col-xs-3">住所</th>
          <td><%= @customer.address %></td>
        </tr>
        <tr>
          <th class="col-xs-3">電話番号</th>
          <td><%= @customer.telephone_number %></td>
        </tr>
        <tr>
          <th class="col-xs-3">メールアドレス</th>
          <td><%= @customer. email%></td>
        </tr>

      </table>
  </div>
